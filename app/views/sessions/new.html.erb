<h1>Login page</h1>


<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] %>
  </div>

<div class="field">
    <%= label_tag :password %>
    <%= password_field :password, params[:password] %>
  </div>

  <!--<div class="actions"><%= submit_tag "Log in" %></div>-->
	<button type = "submit" class = "btn">Submit</button>

<% end %>
